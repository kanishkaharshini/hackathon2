int maxProfit(int* prices, int pricesSize) 
{
    int min = prices[0];
    int profit = 0;
    for (int i = 1; i < pricesSize; i++) 
    {
        if (prices[i] < min) 
        {
            min = prices[i];
        }
        int p = prices[i] - min;
        if (p > profit) 
        {
            profit = p;
        }
    }
    return profit;
}
