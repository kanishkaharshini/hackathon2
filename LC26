int removeDuplicates(int* nums, int numsSize)
{
   if(numsSize == 0) return 0;
    
    int output = 1;
    int loop = numsSize - 1;
    int cnt = 0;
	
    for(int i = 0; i < loop; i++)
    {
        if(nums[i] != nums[i+1])
        {
            output++;
            nums[i+1-cnt] = nums[i+1];
        } else {
            cnt++;
        }
    }
    return output;
}
